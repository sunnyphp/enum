{
    "name": "sunnyphp/enum",
    "description": "Simple AbstractEnum class",
    "type": "library",
    "keywords": [
        "enum",
        "enums",
        "enumeration",
        "enumerations",
        "constant",
        "constants",
        "abstract",
        "abstract class",
        "simple",
        "simplicity"
    ],
    "homepage": "https://github.com/sunnyphp/enum",
    "readme": "readme.md",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Vitaliy Sunny",
            "homepage": "https://github.com/sunnyphp"
        }
    ],
    "support": {
        "issues": "https://github.com/sunnyphp/enum/issues"
    },
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true
    },
    "require": {
        "php": "^7.4 || ^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "SunnyPHP\\Enum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SunnyPHP\\Tests\\Enum\\": "tests/"
        }
    }
}
